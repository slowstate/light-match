; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Light Match"
config/version="0.4"
run/main_scene="res://Menus/MainMenu/main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="3HP/Light Match"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[audio]

general/default_playback_type.web=0
driver/output_latency=5
driver/mix_rate=48000

[autoload]

Globals="*res://Common/globals.gd"
SignalBus="*res://Common/SignalBus/signal_bus.gd"
UpgradeManager="*res://Player/Upgrades/upgrade_manager.gd"
SfxManager="*res://Common/AudioManagers/sfx_manager.tscn"
Logger="*res://Common/Logging/logger.gd"
Config="*res://Common/config.gd"
Utiliies="*res://Common/utiliies.gd"
Settings="*res://Common/settings.gd"
ConditionManager="*res://Arena/Conditions/condition_manager.gd"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/game.log"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/ColorPreview/plugin.cfg", "res://addons/gdLinter/plugin.cfg", "res://addons/gdscript_formatter/plugin.cfg")

[global_group]

Bullets=""
Bots=""
Lizards=""
Tanks=""
Oracles=""
Stars=""

[gui]

theme/custom_font="res://Common/Fonts/exo/Exo-Medium.otf"
theme/default_font_multichannel_signed_distance_field=true

